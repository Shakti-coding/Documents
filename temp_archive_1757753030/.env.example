# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:port/database

# Application Configuration  
PORT=5000
NODE_ENV=production
SESSION_SECRET=your-session-secret-here

# Telegram Bot Configuration
TG_API_ID=your_api_id
TG_API_HASH=your_api_hash
TG_BOT_TOKEN=your_bot_token
TG_AUTHORIZED_USER_ID=your_user_id
TG_SESSION=bot_session
APP_LANGUAGE=en_EN
TG_MAX_PARALLEL=4
TG_DL_TIMEOUT=3600
ENABLED_UNZIP=true
ENABLED_UNRAR=true
TG_PROGRESS_DOWNLOAD=true
YOUTUBE_DEFAULT_DOWNLOAD=VIDEO
YOUTUBE_DEFAULT_EXTENSION=MP4

# Download Path Configuration
TG_DOWNLOAD_PATH=/app/downloads
YOUTUBE_AUDIO_FOLDER=/app/downloads/youtube/audio
YOUTUBE_VIDEO_FOLDER=/app/downloads/youtube/video